{
  "source": {
    "include": ["./src/"]
  },
  "opts": {
    "destination": "./docs/out/",
    "readme": "./README.md",
    "template": "./node_modules/minami"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "idInHeadings": true
  }
}
